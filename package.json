{
  "name": "rest-redux",
  "version": "0.4.1",
  "repository": "https://github.com/nachiket-p/rest-redux",
  "description": "Redux support for REST APIs",
  "main": "lib/index.js",
  "author": "Nachiket Patel",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel --presets es2015,stage-0 -d lib/ src/",
    "prepublish": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "generate-changelog": "./node_modules/github-changes/bin/index.js -o nachiket-p -r rest-redux -n ${npm_package_version} --use-commit-body",
    "preversion": "npm test",
    "version": "npm run generate-changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "normalizr": "^3.2.3",
    "prop-types": "^15.5.10",
    "query-string": "^4.3.4",
    "react": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@types/jest": "^19.2.4",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.23.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "github-changes": "^1.1.0",
    "jest": "^20.0.4",
    "nock": "^9.0.13",
    "redux-mock-store": "^1.2.3",
    "redux-testkit": "^1.0.6",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1"
  },
  "peerDependencies": {
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
